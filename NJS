def compressed(string):
    output = ""
    match_count = 0

    for char in string:
        if not char.isdigit(): 
            if len(output) > 0 and output[-1] == char:
                match_count += 1
            else:
                if match_count > 1:
                    output += str(match_count)
                output += char
                match_count = 1
    if match_count > 1:
          output += str(match_count)
    	    
    return output

string = "aaabccccdd123"
compressed(string)
